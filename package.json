    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "test": "jest",
    "build:android": "npx expo export --platform android",
    "build:ios": "npx expo export --platform ios",
    "build:web": "npx expo export --platform web",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:script": "npm i && npm run build:web && mv dist/_expo/static/js/web/AppEntry* dist/_expo/static/js/web/AppEntry.js && mv dist/_expo dist/assets  ../script/public/ ",
